public class Main {
    public static void main(String[] args) {
    
        int[] dyn = new int[5];
        for (int i = 0; i < 5; i++) dyn[i] = i * i;

        int[] stat = {10, 20, 30, 40, 50};

        System.out.println(Arrays.toString(dyn));
        System.out.println(Arrays.toString(stat));
    }
}
